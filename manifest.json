{
  "manifest_version": 2,
  "name": "Aniwave",
  "version": "1.0",
  "description": "Automatically create watch2gether rooms and send link to discord channel using webhooks",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://aniwave.to/watch2gether/create/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Aniwave Room"
  }
}
